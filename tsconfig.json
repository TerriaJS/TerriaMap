{
  "compilerOptions": {
    "module": "commonjs", 	    
  	"sourceMap": true,
    "target": "ES2015",
    "jsx": "React",
    "types": [ "node" ]
  },
  "forceConsistentCasingInFilenames": true,
  "noImplicitReturns": true,
  "noImplicitUseStrict": true,
  "allowSyntheticDefaultImports": true,
  "include": [
    "app/backend/*"
  ],
  "exclude": [
    "node_modules",
  ]
}